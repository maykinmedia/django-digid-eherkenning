#: backends.py:24
#, python-format
msgid "The %(service)s login from %(ip)s did not succeed or was cancelled."
msgstr "De %(service)s-login van %(ip)s is niet geslaagd of was geannuleerd."

# python-format
#: backends.py:27
#, python-format
msgid "A technical error occurred from %(ip)s during %(service)s login."
msgstr ""
"Er is een technische fout opgetreden tijdens de %(service)s-login vanaf "
"%(ip)s."

# python-format
#: backends.py:30
#, python-format
msgid "User %(user)s%(new_account)s from %(ip)s logged in using %(service)s"
msgstr ""
"Gebruiker %(user)s%(new_account)s is met %(service)s ingelogd vanaf %(ip)s."

#: backends.py:75
msgid "Login failed due to no BSN being returned by DigiD."
msgstr "Login mislukt doordat er geen BSN is teruggegeven door DigiD."

#: backends.py:133
msgid " (new account)"
msgstr " (nieuw account)"

#: backends.py:149
msgid "Login failed due to no RSIN being returned by eHerkenning."
msgstr "Login mislukt doordat er geen RSIN is teruggegeven door eHerkenning."

#: views/digid.py:86
msgid "Login to DigiD did not succeed. Please try again."
msgstr "DigiD-login mislukt. Probeer het nogmaals"

#: views/eherkenning.py:78
msgid "Login to eHerkenning did not succeed. Please try again."
msgstr "eHerkenning-login mislukt. Probeer het nogmaals"
